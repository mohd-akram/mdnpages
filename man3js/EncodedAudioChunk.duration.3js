  type: \(dqkey\(dq,
  data: audioBuffer,
  timestamp: 23000000,
  duration: 2000000,
};
\f[B]const\f[R] chunk = \f[B]new\f[R] EncodedAudioChunk(init);

console.log(chunk.duration); \f[I]// 2000000\f[R]
