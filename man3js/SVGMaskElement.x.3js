\f[B]const\f[R] mask = document.getElementById(\[dq]mask\[dq]);

\f[B]function\f[R] displayLog() {
  \f[B]const\f[R] animValue = mask.x.animVal.value;
  \f[B]const\f[R] baseValue = mask.x.baseVal.value;
