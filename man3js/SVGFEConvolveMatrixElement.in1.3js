      <\f[B]feConvolveMatrix\f[R]
        in=\[dq]SourceGraphic\[dq]
        order=\[dq]3\[dq]
        kernelMatrix=\[dq]0 \-1 0 \-1 4 \-1 0 \-1 0\[dq] />
    </\f[B]filter\f[R]>
  </\f[B]defs\f[R]>
  <\f[B]rect\f[R]
    x=\[dq]20\[dq]
    y=\[dq]20\[dq]
    width=\[dq]100\[dq]
    height=\[dq]100\[dq]
    style=\[dq]fill:lightblue;\[dq]
    filter=\[dq]url(#convolveFilter)\[dq] />
</\f[B]svg\f[R]>
