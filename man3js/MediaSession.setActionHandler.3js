  });
  navigator.mediaSession.setActionHandler(\(dqtogglecamera\(dq, () \f[B]=>\f[R] {
    \f[I]/* Code excerpted. */\f[R]
  });
  navigator.mediaSession.setActionHandler(\(dqtogglemicrophone\(dq, () \f[B]=>\f[R] {
