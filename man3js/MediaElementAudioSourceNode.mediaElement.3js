\f[B]const\f[R] audioElem = document.querySelector(\[dq]audio\[dq]);

\f[B]let\f[R] options = {
  mediaElement: audioElem,
};

\f[B]let\f[R] source = \f[B]new\f[R] MediaElementAudioSourceNode(audioCtx, options);
console.log(source.mediaElement);
