      channels: 2,
      bitrate: 132700,
      samplerate: 5200,
    },
  })
  .then((result) \f[B]=>\f[R] {
    console.log(
      \[ga]This configuration is ${result.supported ? \[dq]\[dq] : \[dq]not \[dq]}supported,\[ga],
    );
    console.log(\[ga]${result.smooth ? \[dq]\[dq] : \[dq]not \[dq]}smooth, and\[ga]);
    console.log(\[ga]${result.powerEfficient ? \[dq]\[dq] : \[dq]not \[dq]}power efficient.\[ga]);
  });
