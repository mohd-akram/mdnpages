  </\f[B]defs\f[R]>
  <\f[B]text\f[R] font\-size=\[dq]20\[dq] fill=\[dq]blue\[dq]>
    <\f[B]textPath\f[R] id=\[dq]myTextPath\[dq] href=\[dq]#myPath\[dq]>
      This text follows a path!
    </\f[B]textPath\f[R]>
  </\f[B]text\f[R]>
</\f[B]svg\f[R]>
