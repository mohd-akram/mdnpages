reader.closed
  .then(() \f[B]=>\f[R] {
    \f[I]// Resolved \- code to handle stream closing\f[R]
  })
  .catch(() \f[B]=>\f[R] {
    \f[I]// Rejected \- code to handle error\f[R]
  });
.EE
