console.log(feBlends[0].in2.baseVal); \f[I]// Output: \[dq]SourceAlpha\[dq]\f[R]
console.log(feBlends[1].in2.baseVal); \f[I]// Output: \[dq]BackgroundImage\[dq]\f[R]
