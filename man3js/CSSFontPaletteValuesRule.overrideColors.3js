\f[B]const\f[R] log = document.getElementById(\(dqlog\(dq);
\f[B]const\f[R] button = document.querySelector(\(dqbutton\(dq);
\f[B]const\f[R] hat = document.querySelector(\(dq.colored\-hat\(dq);
\f[B]const\f[R] rules = document.styleSheets[document.styleSheets.length \- 1].cssRules;
\f[B]const\f[R] greenFontPaletteValuesRule = rules[3];
\f[B]const\f[R] blueFontPaletteValuesRule = rules[2];
log.textContent = \(gaOverridden colors: ${blueFontPaletteValuesRule.overrideColors}\(ga;

button.addEventListener(\(dqclick\(dq, (event) \f[B]=>\f[R] {
  \f[B]if\f[R] (hat.style.fontPalette !== \(dq\-\-green\(dq) {
    hat.style.fontPalette = \(dq\-\-green\(dq;
    log.textContent = \(gaOverridden colors: ${greenFontPaletteValuesRule.overrideColors}\(ga;
  } \f[B]else\f[R] {
    hat.style.fontPalette = \(dq\-\-blue\(dq;
    log.textContent = \(gaOverridden colors: ${blueFontPaletteValuesRule.overrideColors}\(ga;
  }
});
