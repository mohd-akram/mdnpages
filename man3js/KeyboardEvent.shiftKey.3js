  <\f[B]head\f[R]>
    <\f[B]meta\f[R] charset=\(dqutf\-8\(dq />
    <\f[B]meta\f[R] name=\(dqviewport\(dq content=\(dqwidth=device\-width\(dq />
    <\f[B]title\f[R]>shiftKey example</\f[B]title\f[R]>

    <\f[B]script\f[R]>
      \f[B]function\f[R] showChar(e) {
        alert(
          \(dqKey Pressed: \(dq +
            String.fromCharCode(e.charCode) +
            \(dq\(rsn\(dq +
            \(dqcharCode: \(dq +
            e.charCode +
            \(dq\(rsn\(dq +
            \(dqSHIFT key pressed: \(dq +
            e.shiftKey +
            \(dq\(rsn\(dq +
            \(dqALT key pressed: \(dq +
            e.altKey +
            \(dq\(rsn\(dq,
        );
      }
    </\f[B]script\f[R]>
  </\f[B]head\f[R]>

  <\f[B]body\f[R] onkeypress=\(dqshowChar(event);\(dq>
    <\f[B]p\f[R]>
      Press any character key, with or without holding down the SHIFT key.<\f[B]br\f[R] />
      You can also use the SHIFT key together with the ALT key.
    </\f[B]p\f[R]>
  </\f[B]body\f[R]>
</\f[B]html\f[R]>
