\f[B]let\f[R] videoSourceStats = \f[B]null\f[R];

stats.forEach((report) \f[B]=>\f[R] {
  \f[B]if\f[R] (report.type === \(dqmedia\-source\(dq && report.kind===\(dqvideo\(dq) {
    videoSourceStats = report;
    \f[B]break\f[R];
  }
});

\f[B]const\f[R] frames = videoSourceStats?.frames;
