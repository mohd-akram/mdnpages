\f[B]function\f[R] displayData() {
  \f[B]const\f[R] keyRangeValue = IDBKeyRange.bound(\(dqF\(dq, \(dqW\(dq, \f[B]true\f[R], \f[B]true\f[R]);
  console.log(keyRangeValue.lower);

  \f[B]const\f[R] transaction = db.transaction([\(dqfThings\(dq], \(dqreadonly\(dq);
  \f[B]const\f[R] objectStore = transaction.objectStore(\(dqfThings\(dq);

  objectStore.openCursor(keyRangeValue).onsuccess = (event) \f[B]=>\f[R] {
    \f[B]const\f[R] cursor = event.target.result;
    \f[B]if\f[R] (cursor) {
      \f[B]const\f[R] listItem = document.createElement(\(dqli\(dq);
      listItem.textContent = \(ga${cursor.value.fThing}, ${cursor.value.fRating}\(ga;
      list.appendChild(listItem);

      cursor.continue();
    } \f[B]else\f[R] {
      console.log(\(dqEntries all displayed.\(dq);
    }
  };
}
